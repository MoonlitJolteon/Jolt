        #team-info {
          border-radius: 8px 30px;
          width: 435px;
          height: 370px;
          margin-left: 10px;
        }
  
        #team-logo {
          background: #505050d0;
          border-radius: 20px;
          width: 60px;
          height: 60px;
          padding: 10px;
          margin-left: 10px;
          float: left;
        }
        
       .player-icon {
          background: #505050d0;
          border-radius: 20px;
          width: 30px;
          height: 30px;
          margin-left: 10px;
          float: left;
        }
  
        #division-logo {
          background: #505050b9;
          border-radius: 30px;
          width: 55px;
          height: 55px;
          padding: 9px;
