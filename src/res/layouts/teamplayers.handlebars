  <body {{{background}}}>
    <img src={{logoSource}} id="team-logo">
    <img src={{divisionLogo}} id="division-logo">
    <div id="team-info" {{{panel}}} class="inline">
        <h style="padding-top: 5px;">
        &nbsp;&nbsp;Team: {{teamName}}<br>
        {{#if ladder}}
        &nbsp;&nbsp;MMR: {{mmr}}<br>
        {{else}}
        &nbsp;&nbsp;Overall Score: {{score}}<br>
        &nbsp;&nbsp;Change This Cycle: {{cycleScorePM}}<br>
        {{/if}}
        &nbsp;&nbsp;W/L: {{teamWL}}<br>
        &nbsp;&nbsp;Division: {{division}}<br>
        &nbsp;&nbsp;Currently Recruiting: {{recruiting}}<br>
        {{#each players}}
        <img src=https://www.vrmasterleague.com{{this.userLogo}} class="player-icon">
        &nbsp;&nbsp;{{this.playerName}} - {{this.role}}<br>
        {{/each}}
        </h>

    </div>
