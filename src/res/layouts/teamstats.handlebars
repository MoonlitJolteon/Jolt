  #team-info {
    background: #ffffff69;
    border-radius: 8px 30px;
    margin-top: 10px;
    width: 65%;
    height: 150px;
    vertical-align: top;
  }
  #avatar {
    border-radius: 50px;
    width: 150px;
    padding: 10px;
  }
  #team-logo {
    border-radius: 15px;
    width: 80px;
    height: 80px;
    padding: 10px;
    float: left;
  }
  #division-logo {
    border-radius: 30px;
    width: 40px;
