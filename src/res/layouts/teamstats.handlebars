  .inline {
    display: inline-flex;
  }
  #match-info {
    background: #ffffff69;
    border-radius: 8px 30px;
    margin-left: 10px;
    width: 570px;
    height: 280px;
    vertical-align: top;
    text-align: center;
    font-size: 20px;
  }
  .match-info {
    border-style: solid;
    border-radius: 5px;
    border-width: 3px;
    line-height: 1.2em;
    border-color: #505050;
    margin: 5px;
    background: #d8d8d8a2;
  }
  .win {
